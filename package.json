  "scripts": {
    "build:prod": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 webpack --config ./webpack/webpack.prod.ts",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./webpack/webpack.dev.ts",
    "prod": "npm run build:prod && cross-env NODE_ENV=production ts-node server/prod.ts",
    "start": "cross-env NODE_ENV=production ts-node server/prod.ts",
    "test": "jest --maxWorkers=8",
    "test:update": "jest --maxWorkers=8 -u",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "graphql:init": "graphql-codegen init",
    "graphql:generate": "graphql-codegen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
