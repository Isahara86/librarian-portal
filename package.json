{
  "name": "librarian-portal",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.18.0 <17"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@apollo/client": "^3.7.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
